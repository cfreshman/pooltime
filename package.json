{
  "name": "pooltime",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "mongodb": "^3.2.2",
    "nodemon": "^1.18.10",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-scripts": "2.1.8"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "server": "nodemon -w server server/index.js",
    "db-init": "node server/db/init.js",
    "dev-all": ": $(mongod --dbpath=./data) & yarn start & yarn run server"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "proxy": "http://localhost:5000/"
}
